    "@rollup/plugin-typescript": "4.0.0",
    "@types/node": "12.12.35",
    "@types/command-line-args": "5.0.0",
    "@types/command-line-usage": "5.0.1",
    "@types/jest": "25.2.1",
    "@types/mkdirp": "1.0.0",
    "@types/rimraf": "3.0.0",
